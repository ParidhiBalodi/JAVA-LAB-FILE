class Animal {
    void speak() {
        System.out.println("Some animals speak");
    }
}

class Cat extends Animal {
    void speak() {
        System.out.println("Meow");
    }
}

class Cow extends Animal {
    void speak() {
        System.out.println("Moo");
    }
}

public class Main {
    public static void main(String[] args) {
        Animal a1 = new Cat();
        Animal a2 = new Cow();
        a1.speak();
        a2.speak();
    }
}
